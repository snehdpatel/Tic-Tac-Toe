<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:label = "Tic_Tac_Toe"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.sneh.tic_tac_toe.activity_tic__tac__toe">


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_weight="1"
        android:layout_height="fill_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_weight="1"
            android:layout_height="fill_parent">



            <ImageView
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:minHeight="100dp"
                android:minWidth="100dp"
                android:layout_height="wrap_content"
                android:src="@drawable/tic"
                android:id="@+id/main_2_imageView_1" />

            <View
                android:layout_height="5dp"
                android:layout_width="fill_parent"
                android:background="@android:color/darker_gray"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:minHeight="100dp"
                android:minWidth="100dp"
                android:layout_height="wrap_content"
                android:src="@drawable/plain"
                android:id="@+id/main_2_imageView_2" />

            <View
                android:layout_height="5dp"
                android:layout_width="fill_parent"
                android:background="@android:color/darker_gray"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:minHeight="100dp"
                android:minWidth="100dp"
                android:layout_height="wrap_content"
                android:src="@drawable/plain"
                android:id="@+id/main_2_imageView_3" />
        </LinearLayout>

        <View
            android:layout_height="fill_parent"
            android:layout_width="5dp"
            android:background="@android:color/darker_gray"/>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_weight="1"
            android:layout_height="fill_parent">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:minHeight="100dp"
                android:minWidth="100dp"
                android:layout_height="wrap_content"
                android:src="@drawable/plain"
                android:id="@+id/main_2_imageView_4" />

            <View
                android:layout_height="5dp"
                android:layout_width="fill_parent"
                android:background="@android:color/darker_gray"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:minHeight="100dp"
                android:minWidth="100dp"
                android:layout_height="wrap_content"
                android:src="@drawable/tac"
                android:id="@+id/main_2_imageView_5" />

            <View
                android:layout_height="5dp"
                android:layout_width="fill_parent"
                android:background="@android:color/darker_gray"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/plain"
                android:id="@+id/main_2_imageView_6"
                android:layout_gravity="right"
                android:layout_weight="1"
                android:minHeight="100dp"
                android:minWidth="100dp"/>
        </LinearLayout>

        <View
            android:layout_height="fill_parent"
            android:layout_width="5dp"
            android:background="@android:color/darker_gray"/>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_weight="1"
            android:layout_height="fill_parent">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:minHeight="100dp"
                android:minWidth="100dp"
                android:layout_height="wrap_content"
                android:src="@drawable/play"
                android:id="@+id/main_2_imageView_7" />

            <View
                android:layout_height="5dp"
                android:layout_width="fill_parent"
                android:background="@android:color/darker_gray"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:minHeight="100dp"
                android:minWidth="100dp"
                android:layout_height="wrap_content"
                android:src="@drawable/play"
                android:id="@+id/main_2_imageView_8" />

            <View
                android:layout_height="5dp"
                android:layout_width="fill_parent"
                android:background="@android:color/darker_gray"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:minHeight="100dp"
                android:minWidth="100dp"
                android:layout_height="wrap_content"
                android:src="@drawable/toe"
                android:id="@+id/main_2_imageView_9" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_weight="1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"></LinearLayout>
</RelativeLayout>
