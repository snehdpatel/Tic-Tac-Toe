<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sneh.tic_tac_toe" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/index1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Tic_Tac_Toe"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".two_player"
            android:label="@string/title_activity_two_player" >
        </activity>
        <activity
            android:name=".Infinite_tic_tac_toe"
            android:label="@string/title_activity_infinite_tic_tac_toe" >
        </activity>
    </application>

</manifest>
